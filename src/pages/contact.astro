---
import ContactForm from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Sunny Right Now!",
	description:
		"Connect with me to discuss your project and explore web solutions.",
	canonical: "https://sunnisacoder.netlify.app/contact/",
};
---

<BaseLayout seo={seo}>
	<ContactForm />
</BaseLayout>
